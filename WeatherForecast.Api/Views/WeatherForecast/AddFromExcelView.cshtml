@ViewData["Title"] Add weather forecasts from excel
@model WeatherForecast.Api.Requests.AddWeatherForecastFromExelRequest

<h1>@ViewData["Title"]</h1>
<form asp-action="AddFromExcel" method="post" asp-antiforgery="true" enctype="multipart/form-data">
    <p>
        <label asp-for="CityName">Название города</label><br/>
        <input type="text" asp-for="CityName"/>
    </p>
    <p>
        <label asp-for="WeatherForecasts">Фаил</label><br/>
        <input type="file" asp-for="WeatherForecasts"/>
    </p>
    <input type="submit" value="Отправить" />
</form>